{
  "name": "@swan-io/frontend-server",
  "description": "Swan frontend server",
  "version": "1.24.2",
  "private": true,
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": "^9.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/swan-io/frontend"
  },
  "scripts": {
    "build": "pnpm clean && tsc -p tsconfig.json && shx cp -r ./src/views ./dist",
    "clean": "shx rm -rf dist",
    "prepack": "tsc --build",
    "start": "node -r ./dist/tracing.js ./dist/index.js",
    "start-swan": "node -r ./dist/tracing.js ./dist/index.swan.js"
  },
  "dependencies": {
    "@fastify/accepts": "4.3.0",
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/reply-from": "9.8.0",
    "@fastify/secure-session": "7.5.1",
    "@fastify/sensible": "5.6.0",
    "@fastify/static": "7.0.4",
    "@fastify/view": "9.1.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.30.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.57.1",
    "@opentelemetry/instrumentation": "0.57.1",
    "@opentelemetry/instrumentation-fastify": "0.44.1",
    "@opentelemetry/instrumentation-http": "0.57.1",
    "@opentelemetry/instrumentation-pino": "0.46.0",
    "@opentelemetry/propagator-jaeger": "1.30.1",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-trace-base": "1.30.1",
    "@opentelemetry/sdk-trace-node": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.28.0",
    "@swan-io/boxed": "3.2.0",
    "@types/escape-html": "1.0.4",
    "escape-html": "1.0.3",
    "fast-proxy": "2.2.0",
    "fastify": "4.28.1",
    "get-port": "5.1.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "mustache": "4.2.0",
    "node-mailjet": "6.0.6",
    "pathe": "2.0.2",
    "picocolors": "1.1.1",
    "pino-pretty": "11.3.0",
    "ts-pattern": "5.6.0",
    "valienv": "0.7.0"
  },
  "devDependencies": {
    "@types/accepts": "1.3.7",
    "@types/mustache": "4.2.5",
    "graphql": "16.10.0",
    "shx": "0.3.4"
  }
}
