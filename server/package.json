{
  "name": "@swan-io/frontend-server",
  "description": "Swan frontend server",
  "version": "1.24.31",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": "^22.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/swan-io/frontend"
  },
  "scripts": {
    "build": "pnpm clean && tsc -p tsconfig.json && shx cp -r ./src/views ./dist",
    "clean": "shx rm -rf dist",
    "prepack": "tsc --build",
    "start": "node -r ./dist/tracing.js ./dist/index.js",
    "start-swan": "node -r ./dist/tracing.js ./dist/index.swan.js"
  },
  "dependencies": {
    "@fastify/accepts": "5.0.2",
    "@fastify/cors": "11.1.0",
    "@fastify/helmet": "13.0.1",
    "@fastify/middie": "9.0.3",
    "@fastify/reply-from": "12.3.1",
    "@fastify/secure-session": "8.2.0",
    "@fastify/sensible": "6.0.3",
    "@fastify/static": "8.2.0",
    "@fastify/view": "10.0.2",
    "@opentelemetry/auto-instrumentations-node": "0.62.1",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-trace-otlp-proto": "0.203.0",
    "@opentelemetry/propagator-jaeger": "2.0.1",
    "@opentelemetry/sdk-node": "0.203.0",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@swan-io/boxed": "3.2.0",
    "@types/escape-html": "1.0.4",
    "escape-html": "1.0.3",
    "fastify": "5.5.0",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "mustache": "4.2.0",
    "node-mailjet": "6.0.9",
    "pathe": "2.0.3",
    "picocolors": "1.1.1",
    "pino-pretty": "13.1.1",
    "ts-pattern": "5.8.0",
    "valienv": "1.0.0"
  },
  "devDependencies": {
    "@types/accepts": "1.3.7",
    "@types/mustache": "4.2.6",
    "graphql": "16.11.0",
    "jsonc-parser": "3.3.1",
    "shx": "0.4.0",
    "type-fest": "4.41.0"
  }
}
