{
  "name": "@swan-io/frontend-server",
  "description": "Swan frontend server",
  "version": "1.22.13",
  "private": true,
  "engines": {
    "node": ">=18.20.0",
    "yarn": "^1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/swan-io/frontend"
  },
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.json && shx cp -r ./src/views ./dist",
    "clean": "shx rm -rf dist",
    "prepack": "tsc --build",
    "start": "node -r ./dist/tracing.js ./dist/index.js",
    "start-swan": "node -r ./dist/tracing.js ./dist/index.swan.js"
  },
  "dependencies": {
    "@fastify/accepts": "4.3.0",
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/reply-from": "9.8.0",
    "@fastify/secure-session": "7.5.1",
    "@fastify/sensible": "5.6.0",
    "@fastify/static": "6.12.0",
    "@fastify/view": "9.1.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.25.1",
    "@opentelemetry/exporter-jaeger": "1.25.1",
    "@opentelemetry/instrumentation": "0.52.1",
    "@opentelemetry/instrumentation-fastify": "0.38.0",
    "@opentelemetry/instrumentation-http": "0.52.1",
    "@opentelemetry/instrumentation-pino": "0.41.0",
    "@opentelemetry/propagator-jaeger": "1.25.1",
    "@opentelemetry/resources": "1.25.1",
    "@opentelemetry/sdk-trace-base": "1.25.1",
    "@opentelemetry/sdk-trace-node": "1.25.1",
    "@opentelemetry/semantic-conventions": "1.25.1",
    "@swan-io/boxed": "2.3.0",
    "@types/escape-html": "1.0.4",
    "escape-html": "1.0.3",
    "fast-proxy": "2.2.0",
    "fastify": "4.28.1",
    "get-port": "5.1.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "mustache": "4.2.0",
    "node-mailjet": "6.0.5",
    "pathe": "1.1.2",
    "picocolors": "1.0.1",
    "pino-pretty": "11.2.2",
    "ts-pattern": "5.2.0",
    "valienv": "0.7.0"
  },
  "devDependencies": {
    "@types/accepts": "1.3.7",
    "@types/mustache": "4.2.5",
    "graphql": "16.9.0",
    "shx": "0.3.4"
  }
}
