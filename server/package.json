{
  "name": "@swan-io/frontend-server",
  "description": "Swan frontend server",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": "^1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/swan-io/frontend"
  },
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.json && shx cp -r ./src/views ./dist",
    "clean": "shx rm -rf dist",
    "prepack": "tsc --build",
    "start": "node -r ./dist/tracing.js ./dist/index.js",
    "start-swan": "node -r ./dist/tracing.js ./dist/index.swan.js"
  },
  "dependencies": {
    "@fastify/accepts": "4.2.0",
    "@fastify/cors": "8.3.0",
    "@fastify/reply-from": "9.4.0",
    "@fastify/secure-session": "6.2.0",
    "@fastify/sensible": "5.2.0",
    "@fastify/static": "6.10.2",
    "@fastify/view": "7.4.1",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/exporter-jaeger": "1.15.2",
    "@opentelemetry/instrumentation": "0.41.2",
    "@opentelemetry/instrumentation-fastify": "0.32.0",
    "@opentelemetry/instrumentation-http": "0.41.2",
    "@opentelemetry/instrumentation-pino": "0.34.0",
    "@opentelemetry/propagator-jaeger": "1.15.2",
    "@opentelemetry/resources": "1.15.2",
    "@opentelemetry/sdk-trace-base": "1.15.2",
    "@opentelemetry/sdk-trace-node": "1.15.2",
    "@opentelemetry/semantic-conventions": "1.15.2",
    "@swan-io/boxed": "1.1.0",
    "fast-proxy": "2.2.0",
    "fastify": "4.21.0",
    "fastify-language-parser": "3.0.0",
    "get-port": "5.1.1",
    "graphql-request": "5.1.0",
    "graphql-tag": "2.12.6",
    "mustache": "4.2.0",
    "node-mailjet": "6.0.4",
    "pathe": "1.1.1",
    "picocolors": "1.0.0",
    "pino-pretty": "10.2.0",
    "ts-pattern": "5.0.5",
    "valienv": "0.5.0"
  },
  "devDependencies": {
    "@types/accepts": "1.3.5",
    "@types/mustache": "4.2.2",
    "graphql": "16.7.1",
    "shx": "0.3.4"
  }
}
